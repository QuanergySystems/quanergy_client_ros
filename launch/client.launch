<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="ip" default="" />
  <arg name="ns" default="Sensor" />

  <!-- driver -->
  <group ns="$(arg ns)">
    <node name="client_node" 
          pkg="quanergy_client_ros" 
          type="client_node" 
          args="-ip $(arg ip) -settings $(find quanergy_client_ros)/settings/client.xml"
          required="true" 
          output="screen"/>
  </group>
</launch>
